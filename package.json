{
  "author": "R. Mark Volkmann",
  "name": "star-it",
  "version": "1.0.2",
  "description": "A library of JavaScript functions that take an iterable and mimic the functionality of many Array methods",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "compile": "babel -d lib/ src",
    "prepublish": "npm run compile",
    "test": "node_modules/babel/bin/babel.js lib --out-dir build; node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json"
  },
  "keywords": [
    "JavaScript",
    "ES6",
    "iterable",
    "iterator",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mvolkmann/star-it.git"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "jasmine": "2.3.2"
  }
}
